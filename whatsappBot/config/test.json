{
  "App": {
    "resources": {
      "WhatsappBot": {
        "puppeteer": {
          "args": [
            "--no-sandbox",
            "--disable-setuid-sandbox",
            "--unhandled-rejections=strict"
          ],
          "headless": true,
          "executablePath": "/usr/bin/chromium-browser"
        },
        "paths": {
          "qrCodeFile": "qr.png",
          "lastQrFile": "last.qr"
        }
      }
    }
  }
}
